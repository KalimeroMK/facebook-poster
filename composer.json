{
    "name": "laravel-notification-channels/facebook-poster",
    "description": "Use notification to create posts on Facebook",
    "license": "MIT",
    "authors": [{
        "name": "Ahmed Ashraf",
        "email": "ahmed29329@gmail.com"
    }],
    "require": {
        "php": "^7.2",
        "facebook/graph-sdk": "^5.7",
        "illuminate/notifications": "~5.5 || ~6.0",
        "illuminate/support": "~5.5 || ~6.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "orchestra/testbench": "~3.7 || ~4.0",
        "phpunit/phpunit": "~7.0 || ~8.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\FacebookPoster\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\FacebookPoster\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\FacebookPoster\\FacebookPosterServiceProvider"
            ]
        }
    }
}
